{
  "name": "monterey",
  "version": "v0.1.5",
  "description": "Monterey is an extensible application that provides a graphical user interface for a collection of tools to simplify the creation, configuration and maintenance of Aurelia applications.",
  "keywords": [
    "monterey",
    "aurelia",
    "gui"
  ],
  "homepage": "http://aurelia.io",
  "bugs": {
    "url": "https://github.com/monterey-framework/monterey/issues"
  },
  "main": "app/index.js",
  "license": "CC0-1.0",
  "author": {
    "name": "Aurelia-tools",
    "email": "info@aureliatools.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/monterey-framework/monterey"
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist": "build"
  },
  "directories": {
    "app": "app"
  },
  "build": {
    "appId": "monterey",
    "app-category-type": "monterey",
    "win": {
      "iconUrl": "https://i.imgur.com/oiKvnDc.png"
    }
  },
  "devDependencies": {
    "asar": "^0.11.0",
    "aurelia-bundler": "^0.4.0",
    "aurelia-tools": "^0.2.3",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-1": "^6.5.0",
    "changelog": "^1.0.7",
    "conventional-changelog": "1.1.0",
    "del": "^2.2.1",
    "electron-builder": "^5.12.1",
    "electron-connect": "^0.4.2",
    "electron-packager": "^7.2.0",
    "electron-prebuilt": "^1.2.5",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-bump": "^2.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-conventional-changelog": "^1.1.0",
    "gulp-electron-packager": "^1.0.2",
    "gulp-less": "^3.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-protractor": "2.4.0",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-tslint": "^5.0.0",
    "gulp-typescript": "^2.13.6",
    "gulp-zip": "^3.2.0",
    "isparta": "^4.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "jasmine-core": "^2.4.1",
    "jspm": "^0.16.15",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jspm": "2.1.1",
    "mock-require": "^1.3.0",
    "object.assign": "^4.0.3",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.2.1",
    "tslint": "^3.13.0",
    "typescript": ">=1.9.0-dev || ^2.0.0",
    "vinyl-paths": "^2.1.0",
    "yargs": "^4.7.1"
  }
}
